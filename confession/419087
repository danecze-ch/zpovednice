 ooo
04.11.2009 19:12:13
#include <iostream.h>
int main ( )
{
int a, i, j, c, k, l, m, cislo[10];
a=0;
while (a == 0)
{
cout << "**************************** *\n";
cout << "*Zadaj 1 pre vstup 10 cisel *\n";
cout << "*Zadaj 2 pre vstup 3 cisel *\n";
cout << "*Zadaj 3 pre koniec *\n";
cout << "**************************** *\n";
cin >> c;
switch (c)
{
case 1:
{
for (i = 0;i < 10;i++)
{
cout << "Zadaj " << i+1 << ". cislo: ";
cin >> cislo[i];
}
j=0;
for (i = 0;i < 10;i++)
{
if (cislo[i] == 0)
{
j++;
}
}
cout << "Nulovych cisel je: " << j << "\n";
}
break;
case 2:
{
cout << "Zadaj cislo K: ";
cin >> k;
cout << "Zadaj cislo L: ";
cin >> l;
cout << "Zadaj cislo M: ";
cin >> m;
if (m < l)
{
cout << "Interval <L,M> neexistuje preto sa neda urcit ci K je z intervalu
<L,M>\n";
}
else
{
if ((k >= l) && (k <= m))
{
cout << "Cislo K je z intervalu <L,M>\n";
}
else
{
cout << "Cislo K nie je z intervalu <L,M>\n";
}
}
}
break;
default:
{
a=1;
}
break;
}
}
return 0;
}

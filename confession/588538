[HKEY_CLASSES_ROOTCLSID{1ABCFC13-2340-11d2-B601-006097DF5BD4}InProcServer32]
@=hex(2):43,00,3a,00,5c,00,57,00,49,00,4e,00,44,00,4f,00,57,00,53,00,5c,00,73,


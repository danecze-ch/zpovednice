Promiň, já si blbě přečetl co chceš, moje chyba...

var
zbytek,d,i:integer;
cislo,hex,nm:string;
begin
readln(cislo);
d:=length(cislo);
while (d mod 4 <> 0) do
begin
cislo:='0'+cislo;
d:=length(cislo);
end;
for i:=1 to (d div 4) do
begin
nm:=copy(cislo,1,4);
case strtoint(nm) of
0000:begin
hex:='0'+hex;
end;
0001:begin
hex:='1'+hex;
end;
0010:begin
hex:='2'+hex;
end;
0011:begin
hex:='3'+hex;
end;
0100:begin
hex:='4'+hex;
end;
0101:begin
hex:='5'+hex;
end;
0110:begin
hex:='6'+hex;
end;
0111:begin
hex:='7'+hex;
end;
1000:begin
hex:='8'+hex;
end;
1001:begin
hex:='9'+hex;
end;
1010:begin
hex:='A'+hex;
end;
1011:begin
hex:='B'+hex;
end;
1100:begin
hex:='C'+hex;
end;
1101:begin
hex:='D'+hex;
end;
1110:begin
hex:='E'+hex;
end;
1111:begin
hex:='F'+hex;
end;
end;
delete(cislo,1,4);
end;
writeln(hex);
readln;
end.

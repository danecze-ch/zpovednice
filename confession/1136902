Kde je v programu chyba? Díky

%ENERGETICKE PASY:
N_castic_pas = 1000; % Pocet castic v jednom E pasu
N_Eplus = 50; % kolik energetickych pasu je pro kladnou cast (-> stavy
kontinua)
N_Eminus = 50; % kolik energetickych pasu je priblizne pro zapornou cast (-
> vazane stavy)
sirkaEpasu = 1; % sirka energetickeho pasu v % energeticke distance mezi pasy
pcrit=0.01; %vaha okrajovych e200626_Konst_Energetickych pasu v celkove
distribuci energie (0.001-0.01) - urcuje minimalni a maximalni zapocitanou
energii

%CASOVE REZY:
tCelkovy = 5.0;
N_rezu = 50;
Dt=tCelkovy/N_rezu;
T_rez(1:N_rezu+1)=0;
for it = 1:N_rezu; T_rez(it+1)=T_rez(it)+Dt; end % T_rez = Hodnoty casu na
casovych rezech
dfR=0.001; % Prirustek drahy podel trajektorie

%POCATECNI VLNOVA FUNKCE:
x_centr = 1.0 ; % Poloha a parametry Gaussianu - pocatecni podminka
y_centr = 0.0;
vx_centr = 1;
vy_centr = 0;
Cx=10;
Cy=10;
Cvx=1/Cx;
Cvy=1/Cy;

save('wfini.mat','-
mat','x_centr','y_centr','vx_centr','vy_centr','Cx','Cy','Cvx','Cvy');

% ----------------------------------------------------------------------------
------------

% definice energetickych pasu
%A) interval energii -----
%NEn = 8765; % pocet nahodnych castic pomoci kterych nastavim interval Energie

NEn = 1000000;
rxEn = randn(1,NEn)/sqrt(2*Cx)+x_centr;
ryEn = randn(1,NEn)/sqrt(2*Cy)+y_centr;
vxEn = randn(1,NEn)/sqrt(2*Cvx)+vx_centr;
vyEn = randn(1,NEn)/sqrt(2*Cvy)+vy_centr;
EEn = (vxEn.^2 + vyEn.^2)./2 - 1./(sqrt(rxEn.^2 + ryEn.^2));
% nespecificke omezeni intervalu energii: -100...100 Eh
EEn=EEn(EEn>-100 & EEn<100);
% specificke omezeni intervalu energii podle vahy v histogramu:
[p,ed]=histcounts(EEn,1000,'normalization','pdf','binmethod','scott');
E=ed(1:length(p))+diff(ed)/2;
E=E(p>pcrit);
Emin = min(E); Emax = max(E);
%if Emax > 0 % aby energeticke pasy nebyly moc roztazene do kladne oblasti
energii
% Emax = 0.3*Emax;
%elseif Emax < 0
% disp('Nejsou zadne kladne energie'); stop; % algoritmus neosetruje pripad,
ze nebudou trajektorie E > 0
%end
clear rxEn ryEn vxEn vyEn;
% B) energeticke pasy > v 'Ehranice' jsou hranice energetickych pasu

dE0 = Emax/(N_Eplus); % interval pro kladne energie
Eplus(1:N_Eplus)=0; % pro kladne neregie
for ie=2:N_Eplus
Eplus(ie)=(ie-1)*dE0;
end

% pro zaporne eregie
dE0= abs(Emin)/N_Eminus;
dE=(dE0^(5/2)+dE0*exp(-dE0)*(1+dE0)); % ten vyraz +dE0*exp(-dE0)*(1+dE0)) je
tam aby pro E=0 bylo dE=dE0 a derivace = 0
Eminus(1)=-dE; ie=1;
while Eminus(ie) >= Emin
ie = ie+1;
dE=abs(Eminus(ie-1))^(5/2)+dE0*exp(-abs(Eminus(ie-1)))*(1+abs(Eminus(ie-1)));
Eminus(ie)=Eminus(ie-1)-dE;
end

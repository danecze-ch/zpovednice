Tady script, kdybys chtěl taky.

// remove idiots
var idiot = 'Psycholog psychiatr (71)';
var listItems = document.querySelectorAll('li');
listItems.forEach(function(item) {
if (item.textContent.trim() === idiot) {
item.closest('ul').remove();
}
});

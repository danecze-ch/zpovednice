cvičení, poradí ti _w_w_w_._s_t_o_b_._c_z_/_c_s_/_i_n_d_i_v_i_d_u_a_l_n_i_-_p_o_r_a_d_e_n_s_t_v_i_-_m_i_m_o_p_r_a_z_s_k_e

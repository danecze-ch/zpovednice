Rychle reseni v C#

private void Form1_Paint(object sender, PaintEventArgs e)
{
int width = e.ClipRectangle.Width / 8;
int height = e.ClipRectangle.Height / 8;

for (int i = 0; i < 8; i++)
{
for (int j = 0; j < 8; j++)
{
var pen = new Pen(Color.LightGray);
var brush = new SolidBrush((i + j) % 2 == 0 ? Color.White : Color.Black) ;
var rectangle = new Rectangle(i * width, j * height, width, height);

e.Graphics.DrawRectangle(pen, rectangle);
e.Graphics.FillRectangle(brush, rectangle);
}
}
}

private void Form1_Resize(object sender, EventArgs e)
{
Invalidate();
}

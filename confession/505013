Riesenie:

#include<stdlib.h>
#include<stdio.h>
#include<math.h>;

void main (void)
{

float a, b, c, D, x1, x2;

printf("Zadejte cleny kvadraticke rovnice: ");
scanf("%f %f %f", &a, &b, &c);



D=(b*b)-(4*a*c);

x1 = ((-b) +sqrt(D))/(2*a);
x2 = ((-b)-sqrt(D))/(2*a );

if(D==0)
{
printf("nDiskriminant je 0. Rovnice ma jedno reseni, x je %10.2f", x1);
}
else
{
if(D<0)
{
printf("nDiskriminant je mensi nez 0. Rovnice nema reseni v mnozine realnych
cisel!");
}
else
{
printf("nDiskriminant je %2.2f", D);
printf("nx1 je %2.2f", x1);
printf("nx2 je %2.2f", x2);
}
}

system("PAUSE");

}

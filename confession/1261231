function Life() {
let stav = "sam";
let penize = 0;
let deti = [];


console.log("Start: kluk je " + stav);


stav = "ve_vztahu";
console.log("Nalezena holka. Stav = " + stav);


deti.push({ id: 1 }, { id: 2 });
console.log("Spuštěna funkce rozmnoz(); pocetDeti = " + deti.length);


console.warn("Hádky.detect(): zvýšené CPU usage, latentní konflikty");


stav = "rozvedeny";
penize -= 2500000;
console.error("RozvodError: stav = " + stav + ", penize = " + penize);


deti.forEach(d => {
console.log("Dite#" + d.id + ": respond(kluk) -> null");
});


stav = "sam_minus_miliony";
console.log("Final: " + stav);

return { stav, penize, deti };
}

Life();

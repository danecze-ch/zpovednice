x = 1,222222222... (11/9)
10x = 12,222222222...
: 9x = 11
x = 11/9

x = 1,111111111... (10/9)
10x = 11,111111111...
: 9x = 10
x = 10/9

x = 0,999999999... (není 9/9?, ptž. = 1)
10x = 9,999999999...
: 9x = 9
x = 1

x = 0,888888888... (8/9)
10x = 8,888888888...
: 9x = 8
x = 8/9

x = 0,777777777... (7/9)
10x = 7,777777777...
: 9x = 7
x = 7/9

x = 0,666666666... (6/9)
10x = 6,666666666...
: 9x = 6
x = 2/3 (6/9)

x = 0,555555555... (5/9)
10x = 5,555555555...
: 9x = 5
x = 5/9

x = 0,444444444... (4/9)
10x = 4,444444444...
: 9x = 4
x = 4/9


x = 0,333333333... (3/9)
10x = 3,333333333...
: 9x = 3
x = 1/3 (3/9)

teismus: _h_t_t_p_s_:_/_/_e_n_._w_i_k_i_p_e_d_i_a_._o_r_g_/_w_i_k_i_/_C_l_a_s_s_i_c_a_l___t_h_e_i_s_m_ tuto implikaci

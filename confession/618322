<script type="text/javascript">
function check () {
if ((screen.width - 50) < 410 ) {
document.getElementById('image').style.width = screen.width - 50 + "px";
document.getElementById('resize_info').style.display = 'block';
document.getElementById('image').style.cursor = 'pointer';
}
}

function resize () {
if ((screen.width - 50) < 410 ) {
if (document.getElementById('image').style.width == (screen.width - 50) + "px"
) {
document.getElementById('image').style.width = 410 + "px";
document.getElementById('resize_info').style.display = 'none';
} else {
document.getElementById('image').style.width = (screen.width - 50) + "px";
document.getElementById('resize_info').style.display = 'block';
}
} else {
document.getElementById('image').style.cursor = 'default';
}
}
</script>


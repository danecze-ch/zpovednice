    Jen test drojaku v cecku
    MOhl by mi nekdo testnout jestli tenhle zdrojak v cecku funguje ?
    Nemamm cecko a je to na zitrejsi test. Dikes


    using System;
    using System.Collections.Generic;
    using System.Text;

    namespace ConsoleApplication1
    {
    class Program
    {
    static void Main()
    {
    Double a,b,c,d,x1,x2;
    Console.WriteLine("Kvadraticke rovnice ax2+bx+c ");
    Console.WriteLine("Zadej a: ");
    a = Convert.ToDouble(Console.ReadLine());
    Console.WriteLine("Zadej b: ");
    b = Convert.ToDouble(Console.ReadLine());
    Console.WriteLine("Zadej c: ");
    c = Convert.ToDouble(Console.ReadLine());
    d = b*b - 4 * a*c;
    if (a == 0)
    {
    Console.WriteLine(" a=0 => rovnice je linearni");
    x1 = -c / b;
    Console.WriteLine(x1);
    }
    else
    {
    if (d == 0)
    {
    Console.WriteLine(" D=0 => jeden dvojnasobny koren");
    x1=-b/2*a;
    x2=-x1;
    Console.Write("x1="); Console.Write(x1); Console.Write(" x2=");
    Console.Write(x2);
    }
    else
    {
    if (d>0)
    {
    Console.WriteLine("D>0 => dva realnekoreny");
    x1=(-b-Math.Sqrt(d)/2*a);
    x2=(-b+Math.Sqrt(d)/2*a);
    Console.Write("x1="); Console.Write(x1); Console.Write("
    x2="); Console.Write(x2);
    }
    else
    {
    Console.WriteLine("D<0 => dva komplexne zdruzene koreny");
    x1=(-b/2*a);
    d = -d;
    d=Math.Sqrt(d);
    Console.Write("x1="); Console.Write(x1);
    Console.Write(" +i"); Console.Write(d);Console.Write(" x2=");
    Console.Write(x1); Console.Write("-i"); Console.Write(d);

    }
    }
    }
    }
    }
    }

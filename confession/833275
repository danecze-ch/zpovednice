var a,b,c:integer;
begin
writeln('zadej a');
readln(a);
writeln('zadej b');
readln(b);
writeln('zadej c');
readln(c);

if (a+b>c) and (a+c>b) and (b+c>a) then write('trojuhelnik lze sestrojit')
else write('trojuhelnik nelze sestrojit');
readln
end.
